    fun fidentlist() {  stack = Cons(identlist,stack)  }  // SideEffect -> words

    fun fidentdump() {
        var i: Any = identlist
        var a: Any
        var s: String = ""
        var ln: String = ""
        while (i is Cons) {
            a = i.addr
            if (a is Ident) {
                when (a.body) {
                    is Nil  -> s += ln + a.pname
                    is Int  -> s += ln + a.pname + " == " + toValue(a.body)
                    is Cons -> s += ln + a.pname + " == " + toSequence(a.body)
                    else    -> s += ln + a.pname + " == " + "(???)"
                }  }
            ln = "\n"
            i = i.decr
        }
        stack = Cons(s,stack)     // SideEffect -> dump
    }

