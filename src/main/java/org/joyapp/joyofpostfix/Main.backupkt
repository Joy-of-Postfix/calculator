        if (y !is Double) throw Exception(ctfromto + efloatexp)
        val i: Long = Math.round(x)
        var k: Long = Math.round(y)
        var res: Any = Nil()
        if (i <= k)
            while (i <= k) {  res = Cons(k.toDouble(),res) ; k = k - 1  }
        else
            while (i >= k) {  res = Cons(k.toDouble(),res) ; k = k + 1  }
        stack = Cons(res,stack)
    }

