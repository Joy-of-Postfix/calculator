        //if (y !is Ident) throw Exception(ctget + eidentexp)
        var i: Any = x
        var a: Any
        var z: Any = idundef
        while (i is Cons) {
            a = i.addr
            i = i.decr
            if (i !is Cons) throw Exception(ctget + enonvalue)
            if (a == y) {  z = i.addr ; break  }
            i = i.decr
